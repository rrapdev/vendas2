        <addForeignKeyConstraint baseColumnNames="venda_id"
                                 baseTableName="item_venda"
                                 constraintName="fk_item_venda__venda_id"
                                 referencedColumnNames="id"
                                 referencedTableName="venda"/>
    </changeSet>
</databaseChangeLog>
